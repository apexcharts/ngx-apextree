{
  "name": "ngx-apextree-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib && ng serve demo",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "ng build ngx-apextree",
    "build:lib:prod": "ng build ngx-apextree --configuration production",
    "build:demo": "ng build demo",
    "watch": "ng build ngx-apextree --watch",
    "test": "ng test",
    "serve:demo": "ng serve demo"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "apextree": "^1.6.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
